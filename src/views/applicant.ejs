<h1 class="mt-5 mb-4">Jobs</h1>
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">id</th>
            <th scope="col">email</th>
            <th scope="col">Resume</th>
        </tr>
    </thead>
    <tbody>



        <% applicants.forEach(applicant=>{ %>
            <tr>
                <th scope="row">
                    <%= applicant.id %>
                </th>
                <td>
                    <%= applicant.contact %>
                </td>
                <td>
                    <%= applicant.email %>
                </td>

                <td>
                    <img src="<%= applicant.image %>" />
                </td>

            </tr>
         <% }) %>

    </tbody>
</table>